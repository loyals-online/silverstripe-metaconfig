    },
    {
      "name": "Jan-Pieter van der Poel",
      "email": "jp@loyals.nl"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "silverstripe/framework": "^4.0",
    "silverstripe/cms": "^4.0"
